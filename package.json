{
  "name": "pin",
  "version": "1.0.1",
  "description": "",
  "main": "dist/index.js",
  "bin": {
    "pin": "./dist/index.js"
  },
  "scripts": {
    "build": "node ./tasks/build.js",
    "dev": "jest --watch",
    "fmt": "prettier --write",
    "lint": "eslint --fix --ext .ts,.js",
    "prepare": "husky install",
    "test": "jest",
    "types": "tsc --noEmit",
    "precommit": "lint-staged"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "esbuild": "^0.16.17",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "2.8.3",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "*.js": [
      "pnpm fmt",
      "pnpm lint"
    ],
    "*.ts": [
      "pnpm fmt",
      "pnpm lint",
      "pnpm types",
      "bash -c \"pnpm build\""
    ],
    "*.test.ts": [
      "pnpm fmt",
      "pnpm lint",
      "pnpm types",
      "pnpm test"
    ],
    "*.{md, json, yaml}": "pnpm fmt",
    "package.json": [
      "bash -c \"pnpm install\""
    ]
  },
  "engines": {
    "node": ">16.0.0",
    "pnpm": ">=6"
  },
  "dependencies": {
    "axios": "^1.2.3",
    "dotenv": "^16.0.3"
  }
}
